    <Folder Include="DLC\Expansion1\Assets\Gameplay\Data\Policies" />
    <Folder Include="DLC\Expansion1\Assets\Text" />
    <Folder Include="DLC\Expansion1\Assets\Text\en_US" />
    <Folder Include="DLC\Expansion2" />
  </ItemGroup>
  <Import Project="$(MSBuildLocalExtensionPath)Civ6.targets" />
